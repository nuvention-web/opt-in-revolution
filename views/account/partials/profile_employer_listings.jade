.col-md-12
	.page-header
		.prof-label-header
			.row.mt
				.col-md-6
					.fg-h2-special
						h2 My Listings
				.col-md-6
	.opt-login-wrapper
		.table-responsive
			table.table.table-hover
				thead
					tr
						th.proj-title Project Title
						th Date Posted
						th # of Applicants
						th  
				tbody.job-table
					if (joblist.length == 0)
						td You haven't posted any project listings yet!
					else
						each job, i in joblist
							tr
								td.proj-title #{job.jobName}
								td #{job.dateCreated}
								td FAKENUM
								td 
									a.btn.btn-success.btn-md.job-btn(href='/job/viewproject-#{job.id}', role='button') View